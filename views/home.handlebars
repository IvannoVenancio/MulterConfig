    <div class="container">
        <h1>Cadastro de  Cliente</h1>
        <form method="post" action="/cliente" enctype="multipart/form-data">
            
            <label for="nome">Nome:</label>
            <input type="text" id="nome" name="nome" required>

            <label for="idade">Idade:</label>
            <input type="number" id="idade" name="idade" required>

            <label for="morada">Morada:</label>
            <input type="text" id="morada" name="morada" required>

            <label for="morada">Foto:</label>
            <input type="file" id="img" name="photo" required>

            <button type="submit">Adicionar</button>
        </form>
    </div>

    <div class="container">
        <h1>Lista de Clientes</h1>
        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Foto</th>
                    <th>Nome</th>
                    <th>Idade</th>
                    <th>Morada</th>
                </tr>
            </thead>
            <tbody id="clienteTableBody">
                {{#each clientes}}
                <tr>
                    <td>{{id}}</td>
                    <td>
                        <img src="/img/{{photo}}" width="40" height="40" alt="">
                    </td>
                    <td>{{nome}}</td>
                    <td>{{idade}}</td>
                    <td>{{morada}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>